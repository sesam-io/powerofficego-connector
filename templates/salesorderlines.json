[
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-collect",
    "exclude_completeness": [
      "{{@ system @}}-{{@ datatype @}}-share"
    ],
    "namespaced_identifiers": false,
    "source": {
      "dataset": "{{@ system @}}-salesorders-collect",
      "type": "dataset"
    },
    "transform": [
      {
        "rules": {
          "default": [
            [
              "copy",
              "*"
            ],
            [
              "filter",
              [
                "is-not-null",
                "_S.SalesOrderLines"
              ]
            ]
          ]
        },
        "type": "dtl"
      },
      {
        "properties": {
          "allowed_status_codes": "200-299",
          "operation_lookup": "outgoinginvoiceslines_list",
          "operation_lookup_properties": {
            "datatype": "salesorders"
          },
          "primary_key": "Id",
          "rest_system": "{{@ system @}}",
          "share_dataset": "{{@ system @}}-{{@ datatype @}}-share"
        },
        "template": "transform-collect-rest",
        "type": "template"
      },
      {
        "rules": {
          "default": [
            [
              "copy",
              "*"
            ],
            [
              "add",
              "sesam_SalesOrdersId",
              "_S._id"
            ],
            [
              "add",
              "_id",
              "_S.Id"
            ]
          ]
        },
        "type": "dtl"
      }
    ],
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-share",
    "add_namespaces": false,
    "batch_size": 1,
    "compaction": {
      "keep_versions": 100
    },
    "exclude_completeness": [
      "{{@ system @}}-{{@ datatype @}}-share"
    ],
    "namespaced_identifiers": false,
    "sink": {
      "deletion_tracking": false,
      "set_initial_offset": "onload"
    },
    "source": {
      "dataset": "{{@ system @}}-{{@ datatype @}}-transform",
      "type": "dataset"
    },
    "transform": {
      "properties": {
        "operation_delete_properties": {
          "datatype": "salesorders"
        },
        "operation_insert": "{{@ datatype @}}_insert",
        "operation_insert_properties": {
          "datatype": "salesorders"
        },
        "operation_lookup": "{{@ datatype @}}_lookup",
        "operation_lookup_properties": {
          "datatype": "salesorders"
        },
        "primary_key": "Id",
        "rest_system": "{{@ system @}}",
        "share_dataset": "{{@ system @}}-{{@ datatype @}}-share"
      },
      "template": "transform-share-rest",
      "type": "template"
    },
    "type": "pipe"
  }
]